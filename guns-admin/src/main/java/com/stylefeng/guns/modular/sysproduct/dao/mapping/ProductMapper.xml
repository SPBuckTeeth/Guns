<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.sysproduct.dao.ProductMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.sysproduct.model.Product">
        <id column="id" property="id" />
        <result column="enterprise_name" property="enterpriseName" />
        <result column="service_platform" property="servicePlatform" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="status" property="status" />
        <result column="logo" property="logo" />
        <result column="intro" property="intro" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, enterprise_name, service_platform, create_time, update_time, status, logo, intro
    </sql>

</mapper>
